  <DataSets>
    <DataSet Name="ReportHistory">
      <Fields>
        <Field Name="EvaluatedServer">
          <DataField>EvaluatedServer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EvaluationDateTime">
          <DataField>EvaluationDateTime</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="EvaluatedPolicy">
          <DataField>EvaluatedPolicy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EvaluatedObject">
          <DataField>EvaluatedObject</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PolicyResult">
          <DataField>PolicyResult</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="policy_id">
          <DataField>policy_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MonthYear">
          <DataField>MonthYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CategoryName">
          <DataField>CategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FailedPolicyCount">
          <DataField>FailedPolicyCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PolicyDW</DataSourceName>
        <CommandText>SELECT 	EvaluatedServer
	, EvaluationDateTime
	, EvaluatedPolicy
	, EvaluatedObject
	, PolicyResult
	, policy_id
	, CategoryName
	, MonthYear
	, CASE WHEN PolicyResult = 'Fail' THEN 1 ELSE 0 end AS FailedPolicyCount
FROM policy.v_PolicyHistory_LastEvaluation
WHERE CategoryName IN (@CategoryName)
AND (EvaluatedServer IN (SELECT	server_name
		FROM policy.pfn_ServerGroupInstances(@ServerGroupName))
OR @ServerGroupName = '')</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CategoryName">
            <Value>=Parameters!CategoryName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerGroupName">
            <Value>=Parameters!ServerGroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_CategoryName">
      <Fields>
        <Field Name="CategoryName">
          <DataField>CategoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PolicyDW</DataSourceName>
        <CommandText>SELECT DISTINCT CategoryName
FROM policy.v_PolicyHistory</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_MonthYear">
      <Fields>
        <Field Name="MonthYear">
          <DataField>MonthYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MonthStart">
          <DataField>MonthStart</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PolicyDW</DataSourceName>
        <CommandText>SELECT DISTINCT DATEADD(d,-day(EvaluationDateTime)+1, CAST(EvaluationDateTime AS DATE)) AS MonthStart
,  MonthYear
FROM policy.v_PolicyHistory
UNION
SELECT '1900-01-01', 'ALL'
ORDER BY MonthStart DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="ReportHistoryFull">
      <Fields>
        <Field Name="FailedPolicyCount">
          <DataField>FailedPolicyCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MonthStart">
          <DataField>MonthStart</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="MonthYear">
          <DataField>MonthYear</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PolicyDW</DataSourceName>
        <CommandText>SELECT 	MonthYear
	, DATEADD(d,-day(EvaluationDateTime)+1, CAST(EvaluationDateTime AS DATE)) AS MonthStart
	, CASE WHEN PolicyResult IN ('Fail', 'Error') THEN 1 ELSE 0 end AS FailedPolicyCount
FROM policy.v_PolicyHistory
WHERE CategoryName IN (@CategoryName)
AND (EvaluatedServer IN (SELECT	server_name
		FROM policy.pfn_ServerGroupInstances(@ServerGroupName))
OR @ServerGroupName = '')
AND (EvaluationDateTime &gt;= DATEADD(d,-day(DateAdd(Month, -6, @DateMonth))+1, CAST(DateAdd(Month, -6, @DateMonth) AS DATE))
AND EvaluationDateTime &lt; DATEADD(s,-1,DATEADD(mm, DATEDIFF(m,0,@DateMonth)+1,0))
OR (EvaluationDateTime &gt;= DATEADD(d,-day(DateAdd(Month, -6, GETDATE()))+1, CAST(DateAdd(Month, -6, GETDATE()) AS DATE))
AND @DateMonth = '1900-01-01'))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CategoryName">
            <Value>=Parameters!CategoryName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateMonth">
            <Value>=Parameters!DateMonth.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerGroupName">
            <Value>=Parameters!ServerGroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="FailedInstances">
      <Fields>
        <Field Name="InstanceStatus">
          <DataField>InstanceStatus</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EvaluatedServer">
          <DataField>EvaluatedServer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PolicyDW</DataSourceName>
        <CommandText>SELECT CASE
	WHEN EXISTS
		(SELECT *
		FROM policy.v_PolicyHistory_LastEvaluation iple
		WHERE iple.EvaluatedServer = ple.EvaluatedServer
		AND iple.PolicyResult = 'fail')
	THEN 'Out of Compliance'
	ELSE 'In Compliance'
	END AS InstanceStatus
	, EvaluatedServer
FROM policy.v_PolicyHistory ple
WHERE (EvaluatedServer IN (SELECT	server_name
		FROM policy.pfn_ServerGroupInstances(@ServerGroupName))
OR @ServerGroupName = '')
GROUP BY EvaluatedServer</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ServerGroupName">
            <Value>=Parameters!ServerGroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Evaluation_Errors">
      <Fields>
        <Field Name="ErrorHistoryID">
          <DataField>ErrorHistoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PolicyDW</DataSourceName>
        <CommandText>SELECT	ErrorHistoryID
FROM	policy.v_EvaluationErrorHistory_LastEvaluation
WHERE EvaluationOrderDesc = 1
AND CategoryName IN (@CategoryName)
AND (EvaluatedServer IN (SELECT	server_name
		FROM policy.pfn_ServerGroupInstances(@ServerGroupName))
OR @ServerGroupName = '')</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CategoryName">
            <Value>=Parameters!CategoryName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ServerGroupName">
            <Value>=Parameters!ServerGroupName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Parameter_ServerGroup">
      <Fields>
        <Field Name="GroupName">
          <DataField>GroupName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GroupValue">
          <DataField>GroupValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>PolicyDW</DataSourceName>
        <CommandText>SELECT GroupName, GroupValue
FROM policy.v_ServerGroups
ORDER BY Sort</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
